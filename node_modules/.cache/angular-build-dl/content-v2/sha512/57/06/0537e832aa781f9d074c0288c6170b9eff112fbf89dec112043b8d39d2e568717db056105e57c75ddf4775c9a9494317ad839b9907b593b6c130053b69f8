s();\n  }\n\n  // get interest\n  getInterest = () => {\n    // this.srvAccountService.getUserInterestsEndpoint().pipe(takeUntil(this.unsubscribe)).subscribe(res => {\n\n    //   this.interestList = res['response'].data.interests;\n\n    // });\n\n\n    this.srvAccountService.getInterestList().subscribe(res => {\n      // console.log(res['response'].data);\n      this.obj = res['response'].data;\n      // console.log(this.obj.interest);\n\n      for (let ob in this.obj) {\n\n\n        this.interestList1 = this.obj[ob];\n        break;\n      }\n\n      for (let es of this.interestList1) {\n        // console.log(es.interest_name);\n        this.list.push(es.interest_name);\n      }\n      // console.log(this.list);\n      this.srvLocalStorageFactory.writeInterestObjecttoLocalStorage(this.list);\n    });\n\n\n    // console.log(this.list);\n  }\n  // fuc get cities from backend\n  getCities = () => {\n    this.srvAccountService.getUKanTargetedCities().pipe(takeUntil(this.unsubscribe)).subscribe(res => {\n      this.citiesList = res['response'].data['cities'];\n      // console.log(this.citiesList);\n    });\n  }\n  // hide host activity area if on search \n  ngAfterContentChecked(): void {\n    let res = this.router.url.substr(0, 7);\n    if (res !== '/search') {\n      this.hostActivity = true;\n    } else {\n      this.hostActivity = false;\n    }\n  }\n  // navigate to search component to search desired contents of user\n  search = (data, type) => {\n\n    if (type === 'city') {\n      this.searchInputCity = data;\n    } else {\n      for (var i = 0; i < this.list.length; i++) {\n        if (this.list[i] === data) {\n          // console.log(data);\n          // console.log(i);\n          this.searchInputInterestIndex.push(i);\n        }\n      }\n      this.searchInputInterest.push(data);\n    }\n    // this.searchInputDate = moment(this.searchInputDate).format(\"YYYY-MM-DD\");\n    if (this.searchInputCity === \"City\") {\n      this.searchInputCity = \"\";\n    }\n    this.router.navigate([\"/activities\"], {\n      queryParams: {\n        interest: this.searchInputInterest,\n        interestIndex: this.searchInputInterestIndex,\n        date: this.searchInputDates,\n        city: this.searchInputCity,\n        title: this.searchInputText\n      },\n      queryParamsHandling: \"merge\"\n    });\n  }\n\n  licenseAgreement = () => {\n\n    this.router.navigate(['license-agreement']);\n  }\n  pivacyPolicy = () => {\n    this.router.navigate(['privacy-policy']);\n  }\n  contentPolicy = () => {\n    this.router.navigate(['content-policy']);\n  }\n  refundPolicy = () => {\n    this.router.navigate(['refund-policy']);\n  }\n  FacebookLink() {\n    window.open('https://www.facebook.com/youcanllc/', \"_blank\")\n  }\n  TwitterLink() {\n    window.open('https://twitter.com/YoucanUSA', \"_blank\");\n  }\n  LinkedinLink() {\n    window.open('https://www.linkedin.com/company/youcanllc/', \"_blank\");\n  }\n  InstagramLink() {\n    window.open('https://www.instagram.com/youcanusa/', \"_blank\");\n  }\n  PlayStoreLink() {\n    window.open('https://play.google.com/store/apps/details?id=com.youcan.application&hl=en', \"_blank\");\n  }\n  AppStoreLink() {\n    window.open('https://itunes.apple.com/us/app/youcan/id1326029415?ls=1&mt=8', \"_blank\");\n  }\n  // func to navigate to search page \n  navigateToSearchPage = () => {\n    this.router.navigate(['/search']);\n  }\n  // fuc to navigate user to register page \n  navigateToCreateActivityPage = () => {\n\n    if (this.srvJwthelperService.verifyToken()) {\n      this.router.navigate(['/create-activity']);\n    } else {\n      this.router.navigate([UkanAppRoutes.LOGIN]);\n    }\n  }\n  ngOnDestroy(): void {\n    this.unsubscribe.next();\n    this.unsubscribe.complete();\n  }\n}\n","<!-- <div class=\"row\" *ngIf=\"hostActivity\">\n  <div class=\"col-12 host-activity\">\n    <div class=\"text-center mt-4 p1\">\n      Easy to use platform for hosting activities\n    </div>\n    <div class=\"t