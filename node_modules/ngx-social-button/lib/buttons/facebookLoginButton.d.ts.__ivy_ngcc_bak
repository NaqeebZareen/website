import { OnInit, EventEmitter } from '@angular/core';
import { SocialService } from '../social.service';
import { SocialUser } from '../entities';
export declare class FacebookLoginButton implements OnInit {
    private socialAuthService;
    size: string;
    text: string;
    style: string;
    scopes: [string];
    socialUser: EventEmitter<SocialUser>;
    constructor(socialAuthService: SocialService);
    ngOnInit(): void;
    socialSignIn(): void;
}
